{
	"name": "inline-local-graph",
	"version": "0.9.2",
	"description": "A plugin that allows you to visualize the local graph inline within your notes.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"obsidian",
		"graph",
		"local graph",
        "inline graph",
        "inline local graph",
		"plugin"
	],
	"author": "TKOxff <tkoxff@gmail.com>",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"vis-network": "^10.0.1"
	}
}
